using UnityEngine;
using System.Collections;


public class LaserAI : MonoBehaviour {
  
	public float speed;
	
	// Use this for initialization
	void Start () {
		Vector3 newVelocity = Vector3.zero;
		newVelocity.y = speed;
		rigidbody.velocity = newVelocity;
	}
	
	// Update is called once per frame
	void Update () {
	}
}
